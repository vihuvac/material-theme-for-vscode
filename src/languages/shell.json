      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Shell Punctuation",
      "scope": [
        "punctuation.definition.string.begin.shell",
        "punctuation.definition.string.end.shell",
        "punctuation.separator.statement.and.shell",
        "punctuation.terminator.statement.semicolon.shell"
      ],
      "settings": {
