{
  "tokenColors": [
    {
      "name": "Shell Arguments",
      "scope": [
        "string.unquoted.argument.shell"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Shell Script",
      "scope": [
        "constant.other.option.dash.shell"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
