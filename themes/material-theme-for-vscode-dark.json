{
  "name": "material-theme-for-vscode",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#EEFFFF",
    "editorCursor.foreground": "#FFCC00",
    "editor.selectionBackground": "#546E7A80",
    "editor.lineHighlightBackground": "#314549",
    "editorGutter.background": "#263238",
    "editorGutter.modifiedBackground": "#FFCB6B",
    "editorGutter.addedBackground": "#C3E88D",
    "editorGutter.deletedBackground": "#FF5370",
    "activityBar.background": "#1E272C",
    "activityBar.foreground": "#EEFFFF",
    "activityBarBadge.background": "#007ACC",
    "activityBarBadge.foreground": "#EEFFFF",
    "sideBar.background": "#1E272C",
    "sideBar.foreground": "#EEFFFF",
    "sideBarTitle.foreground": "#BBBBBB",
    "statusBar.background": "#1E272C",
    "statusBar.foreground": "#EEFFFF",
    "statusBar.noFolderBackground": "#1E272C",
    "statusBar.debuggingBackground": "#FF5370",
    "tab.activeBackground": "#314549",
    "tab.inactiveBackground": "#1E272C",
    "tab.activeForeground": "#EEFFFF",
    "tab.inactiveForeground": "#BBBBBB"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "JSDoc Comments",
      "scope": [
        "comment.block.documentation",
        "comment.block.documentation.jsdoc"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "JSDoc Tags",
      "scope": [
        "entity.name.tag.jsdoc",
        "keyword.other.jsdoc"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "JSDoc Types",
      "scope": [
        "support.type.jsdoc"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Punctuation in JSDoc, Yaml, and XML",
      "scope": [
        "punctuation.definition.block.tag.jsdoc",
        "punctuation.definition.block.sequence.item.yaml",
        "punctuation.definition.string.begin.yaml",
        "punctuation.definition.string.end.yaml",
        "punctuation.separator.key-value.mapping.yaml",
        "punctuation.definition.tag.xml",
        "punctuation.definition.string.begin.xml",
        "punctuation.definition.string.end.xml"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Javascript/Typescript",
      "scope": [
        "support.type.object.module.js",
        "support.type.object.module.ts"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "TypeScript Types",
      "scope": [
        "support.type.primitive.ts",
        "support.type.builtin.ts"
