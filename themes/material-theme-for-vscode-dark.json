{
  "name": "material-theme-for-vscode",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#EEFFFF",
    "editorCursor.foreground": "#FFCC00",
    "editor.selectionBackground": "#546E7A80",
    "editor.lineHighlightBackground": "#314549",
    "editorGutter.background": "#263238",
    "editorGutter.modifiedBackground": "#FFCB6B",
    "editorGutter.addedBackground": "#C3E88D",
    "editorGutter.deletedBackground": "#FF5370",
    "activityBar.background": "#1E272C",
    "activityBar.foreground": "#EEFFFF",
    "activityBarBadge.background": "#007ACC",
    "activityBarBadge.foreground": "#EEFFFF",
    "sideBar.background": "#1E272C",
    "sideBar.foreground": "#EEFFFF",
    "sideBarTitle.foreground": "#BBBBBB",
    "statusBar.background": "#1E272C",
    "statusBar.foreground": "#EEFFFF",
    "statusBar.noFolderBackground": "#1E272C",
    "statusBar.debuggingBackground": "#FF5370",
    "tab.activeBackground": "#314549",
    "tab.inactiveBackground": "#1E272C",
    "tab.activeForeground": "#EEFFFF",
    "tab.inactiveForeground": "#BBBBBB"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "JSDoc Comments",
      "scope": ["comment.block.documentation", "comment.block.documentation.jsdoc"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "JSDoc Tags",
      "scope": ["entity.name.tag.jsdoc", "keyword.other.jsdoc"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "JSDoc Types",
      "scope": ["support.type.jsdoc"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Punctuation in JSDoc, Yaml, and XML",
      "scope": [
        "punctuation.definition.block.tag.jsdoc",
        "punctuation.definition.block.sequence.item.yaml",
        "punctuation.definition.string.begin.yaml",
        "punctuation.definition.string.end.yaml",
        "punctuation.separator.key-value.mapping.yaml",
        "punctuation.definition.tag.xml",
        "punctuation.definition.string.begin.xml",
        "punctuation.definition.string.end.xml"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "support.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Javascript",
      "scope": ["support.type.object.module.js"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "TypeScript Types",
      "scope": ["support.type.primitive.ts", "support.type.builtin.ts"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "TypeScript Interfaces",
      "scope": ["entity.name.type.interface.ts"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "TypeScript Generics",
      "scope": ["meta.type.parameters.ts"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "React JSX Tags",
      "scope": [
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "name": "React JSX Attributes",
      "scope": [
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.jsx"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Punctuation in JavaScript/TypeScript",
      "scope": [
        "punctuation.accessor.js",
        "punctuation.accessor.jsx",
        "punctuation.accessor.ts",
        "punctuation.accessor.tsx",
        "punctuation.definition.string.begin.js",
        "punctuation.definition.string.begin.jsx",
        "punctuation.definition.string.begin.ts",
        "punctuation.definition.string.begin.tsx",
        "punctuation.definition.string.end.js",
        "punctuation.definition.string.end.jsx",
        "punctuation.definition.string.end.ts",
        "punctuation.definition.string.end.tsx",
        "punctuation.definition.string.template.begin.js",
        "punctuation.definition.string.template.begin.jsx",
        "punctuation.definition.string.template.begin.ts",
        "punctuation.definition.string.template.begin.tsx",
        "punctuation.definition.string.template.end.js",
        "punctuation.definition.string.template.end.jsx",
        "punctuation.definition.string.template.end.ts",
        "punctuation.definition.string.template.end.tsx",
        "punctuation.definition.tag.begin.jsx",
        "punctuation.definition.tag.end.jsx",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx",
        "punctuation.separator.comma.js",
        "punctuation.separator.comma.jsx",
        "punctuation.separator.comma.ts",
        "punctuation.separator.comma.tsx",
        "punctuation.separator.key-value.js",
        "punctuation.separator.key-value.jsx",
        "punctuation.separator.key-value.ts",
        "punctuation.separator.key-value.tsx",
        "punctuation.terminator.statement.js",
        "punctuation.terminator.statement.jsx",
        "punctuation.terminator.statement.ts",
        "punctuation.terminator.statement.tsx"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Templates in Javascript/TypeScript",
      "scope": [
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.end.ts"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Angular Decorators",
      "scope": ["meta.decorator.angular", "entity.name.decorator.angular"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Angular Templates",
      "scope": ["text.html.angular"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["text.html.basic entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["source.css support.type.property-name"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown Punctuation",
      "scope": [
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.end.markdown",
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Markdown Separators",
      "scope": ["punctuation.separator.key-value.html"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "JSON Keys Quotes",
      "scope": [
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "JSON Values Quotes",
      "scope": [
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.separator.array.json.comments"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "JSON Strings",
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "JSON Numbers",
      "scope": ["constant.numeric.json"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "JSON Booleans",
      "scope": ["constant.language.json"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "JSON Null",
      "scope": ["constant.language.null.json"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Yaml Booleans",
      "scope": ["constant.language.boolean.yaml"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Yaml Null",
      "scope": ["constant.language.null.yaml"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Shell Arguments",
      "scope": [
        "string.unquoted.argument.shell"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Shell Script",
      "scope": [
        "constant.other.option.dash.shell"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Shell Arguments",
      "scope": [
        "constant.other.option",
        "punctuation.definition.subshell.single.shell"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Shell Punctuation",
      "scope": [
        "punctuation.definition.string.begin.shell",
        "punctuation.definition.string.end.shell",
        "punctuation.separator.statement.and.shell",
        "punctuation.terminator.statement.semicolon.shell"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Python Keywords",
      "scope": ["source.python keyword"],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Python Imports",
      "scope": ["source.python meta.import"],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Python Functions",
      "scope": ["source.python entity.name.function"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Python Inner Variables (Parameters)",
      "scope": ["source.python variable.parameter"],
      "settings": {
        "foreground": "#F67B56"
      }
    },
    {
      "name": "Python Strings",
      "scope": [
        "source.python string",
        "punctuation"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Python Numbers",
      "scope": ["source.python constant.numeric"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Python Comments",
      "scope": ["source.python comment"],
      "settings": {
        "foreground": "#546E7A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Class Names",
      "scope": ["source.python entity.name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Python Decorators",
      "scope": ["source.python meta.decorator"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Python Built-in Types",
      "scope": ["source.python support.type"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "C# Keywords",
      "scope": ["source.cs keyword"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "C# Regions",
      "scope": [
        "keyword.preprocessor.region.cs",
        "keyword.preprocessor.endregion.cs",
        "meta.preprocessor.region.cs",
        "meta.preprocessor.endregion.cs",
        "string.unquoted.preprocessor.message.cs"
      ],
      "settings": {
        "foreground": "#546E7A"
      }
    },
    {
      "name": "C# Types",
      "scope": ["source.cs entity.name.type"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "C# Methods",
      "scope": ["source.cs entity.name.function"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "C# Strings",
      "scope": ["source.cs string"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "C# Numbers",
      "scope": ["source.cs constant.numeric"],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "C# Comments",
      "scope": ["source.cs comment"],
      "settings": {
        "foreground": "#546E7A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C# Attributes",
      "scope": ["source.cs meta.attribute"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "C# Control Flow",
      "scope": ["source.cs keyword.control"],
      "settings": {
        "foreground": "#89DDFF"
      }
    }
  ]
}
